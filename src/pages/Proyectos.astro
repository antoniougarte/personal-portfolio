---
import SectionContainer from "../components/SectionContainer.astro";
import Layout from "../layouts/Layout.astro";
import CodeIcon from "../components/icons/Code.astro"
import Projects from "../components/Projects.astro"
---
<Layout
  title="Proyectos de Paulo Antonio - Desarrollador Web e Ingeniero de software"
  description="Paulo Antonio. Desarrollador web e Ingeniero de software, capacitado para crear aplicaciones y/o aprender nuevas tecnologias para aportar en tus proyectos"
>
<main class="px-4">
  <SectionContainer class="py-44 pb-32" id="proyectos">
    <h2 class="text-3xl font-semibold mb-6 flex gap-x-3 items-center">
      <CodeIcon class="size-7 text-gray-200" />
        <span class="text-gray-200">Proyectos</span>
    </h2>
    <Projects />
  </SectionContainer>
</main>
</Layout>